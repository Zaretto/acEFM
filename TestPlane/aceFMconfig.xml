<?xml version="1.0"?>
<PropertyList>
  <fdm>
    <jsbsim>
      <fcs>
        <yaw-damper-enable />
      </fcs>
    </jsbsim>
  </fdm>
  <sim>
    <description>A-4E</description>
    <long-description>
            A-4E
            long words
        </long-description>
    <status>production</status>

    <aero>A-4E-vsp</aero>
    <current-view>
      <internal></internal>
    </current-view>
    <!-- bind all cockpit params -->
    <cockpit>
      <gauge>
        <param>Airspeed</param>
        <property>/fdm/jsbsim/velocities/vc-kts</property>
      </gauge>
      <gauge>
        <param>MachIndicator</param>
        <property>/fdm/jsbsim/velocities/mach</property>
      </gauge>
      <gauge>
        <param>Slipball</param>
        <property>/fdm/jsbsim/aero/beta-deg</property>
        <factor>DEGREES_TO_RADIANS</factor>
      </gauge>
      <gauge>
        <param>Variometer</param>
        <property>/fdm/jsbsim/velocities/vsi-instant-ftmin</property>
      </gauge>
      <gauge>
        <param>AI_Pitch</param>
        <property>/fdm/jsbsim/attitude/pitch-rad</property>
      </gauge>
      <gauge>
        <param>AI_Bank</param>
        <property>/fdm/jsbsim/attitude/roll-rad</property>
      </gauge>
      <gauge>
        <param>SAI_Pitch</param>
        <property>/fdm/jsbsim/attitude/pitch-rad</property>
      </gauge>
      <gauge>
        <param>SAI_Bank</param>
        <property>/fdm/jsbsim/attitude/roll-rad</property>
      </gauge>
      <gauge>
        <param>HSI_CompassCard</param>
        <property>/fdm/jsbsim/attitude/psi-rad</property>
      </gauge>
      <gauge>
        <param>Flight_Pressure</param>
        <property>/fdm/jsbsim/systems/hydraulics/number1-system-pump-psi</property>
      </gauge>
      <gauge>
        <param>Utility_Pressure</param>
        <property>/fdm/jsbsim/systems/hydraulics/number2-system-pump-psi</property>
      </gauge>
      <gauge>
        <param>AOA_Units</param>
        <property>/fdm/jsbsim/aero/alpha-deg</property>
      </gauge>
      <gauge>
        <param>Tachometer_Left</param>
        <property>/fdm/jsbsim/propulsion/engine/n2</property>
      </gauge>
      <gauge>
        <param>Tachometer_percent_Left</param>
        <property>/fdm/jsbsim/propulsion/engine/n1</property>
      </gauge>
      <gauge>
        <param>EGT_Left</param>
        <property>/fdm/jsbsim/propulsion/engine/EGT-C</property>
      </gauge>
      <gauge>
        <param>OilPressure_Left</param>
        <property>/fdm/jsbsim/propulsion/engine[0]/oil-pressure-psi</property>
      </gauge>
      <gauge>
        <param>NozzlePos_Left</param>
        <property>/fdm/jsbsim/propulsion/engine[0]/alt/nozzle-pos-norm</property>
      </gauge>
      <gauge>
        <param>CabinPressure</param>
        <property>/fdm/jsbsim/systems/ecs/cabin-altitude-ft</property>
        <factor>0.001</factor>
      </gauge>
      <gauge>
        <param>FuelFlow_Left</param>
        <property>/fdm/jsbsim/propulsion/engine/fuel-flow-rate-pps</property>
        <factor>3600</factor>
      </gauge>
      <gauge>
        <param>FuelQuantity_Left</param>
        <property>/fdm/jsbsim/propulsion/total-fuel-l-lbs</property>
      </gauge>
      <gauge>
        <param>FuelQuantity_Right</param>
        <property>/fdm/jsbsim/propulsion/total-fuel-r-lbs</property>
      </gauge>
      <gauge>
        <param>OxygenQuantity</param>
        <property>/fdm/jsbsim/systems/ecs/oxygen-quantity-liters</property>
      </gauge>
      <gauge>
        <param>Accelerometer</param>
        <property>accelerations/wdot-ft_sec2</property>
        <factor>0.03125</factor>
      </gauge>

      <gauge>
        <type>GenevaDrive</type>
        <param>Altimeter_10000_footCount</param>
        <property>fdm/jsbsim/position/h-sl-ft</property>
        <factor>10000</factor>
      </gauge>
      <gauge>
        <type>GenevaDrive</type>
        <param>Altimeter_1000_footCount</param>
        <property>fdm/jsbsim/position/h-sl-ft</property>
        <factor>1000</factor>
      </gauge>
      <gauge>
        <type>GenevaDrive</type>
        <param>Altimeter_100_footCount</param>
        <property>fdm/jsbsim/position/h-sl-ft</property>
        <factor>100</factor>
      </gauge>
      <gauge>
        <type>LinearDrive</type>
        <param>Altimeter_100_footPtr</param>
        <property>fdm/jsbsim/position/h-sl-ft</property>
        <factor>100</factor>
      </gauge>
    </cockpit>
  </sim>
</PropertyList>
